<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|ARM'">
    <RemoteDebuggerCommand>c:\deploy\HologramSDK.exe</RemoteDebuggerCommand>
    <RemoteDebuggerWorkingDirectory>c:\deploy</RemoteDebuggerWorkingDirectory>
    <RemoteDebuggerServerName>HologramWinIoT:8116</RemoteDebuggerServerName>
